
{
  "manifest_version": 3,
  "name": "PassMan - Password Manager",
  "version": "1.0",
  "description": "A Simple yet powerful Password Manager",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["storage", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
